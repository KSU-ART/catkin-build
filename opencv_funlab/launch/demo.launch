<launch>
	<node name="usb_cam" pkg="usb_cam" type="usb_cam_node" output="screen" >
           <param name="video_device" value="/dev/video1" />
           <param name="image_width" value="640" />
           <param name="image_height" value="480" />
           <param name="pixel_format" value="yuyv" />
           <param name="camera_frame_id" value="usb_cam" />
           <param name="io_method" value="mmap"/>
	</node>
	<node pkg="opencv_funlab" type="opencv_fun_tracking_lab_node" name="opencv_fun_tracking_lab_node"/>
	<node pkg="opencv_funlab" type="plate_angle" name="plate_angle"/>
	<node name="image_view" type="image_view" pkg="image_view">
		<remap from="image" to="/usb_cam/image_tracked/"/>
	</node>	
	<node pkg="opencv_funlab" type="opencv_fun_subscriber_node" name="opencv_fun_subscriber_node" output="screen"/>
</launch>
